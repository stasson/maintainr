{
  "name": "maintainr",
  "version": "0.0.6",
  "description": "misc repo maintainer utils",
  "author": {
    "name": "stasson",
    "email": "sebastien.tasson@gmail.com"
  },
  "bin": "bin/maintainr.js",
  "main": "lib/index",
  "types": "lib/index",
  "files": [
    "lib",
    "bin"
  ],
  "scripts": {
    "prepare": "rimraf lib && tsc",
    "start": "rimraf lib && tsc && node bin/maintainr.js",
    "test": "run-s test:*",
    "test:lint": "cross-env NODE_ENV=production tslint src/**/*.ts -t verbose",
    "format": "prettier --write src/**/*",
    "commit": "standard-commit",
    "release": "run-s release:*",
    "release:checks": "run-s test:*",
    "release:version": "standard-version",
    "release:publish": "git push --follow-tags && npm publish"
  },
  "gitHooks": {
    "commit-msg": "standard-commithook"
  },
  "license": "MIT",
  "devDependencies": {
    "@types/fs-extra": "^8.0.1",
    "@types/jest": "^24.0.20",
    "@types/js-yaml": "^3.12.1",
    "@types/node": "^10.17.0",
    "@types/normalize-package-data": "^2.4.0",
    "@types/semver": "^6.2.0",
    "cross-env": "^6.0.3",
    "jest": "^24.9.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^1.18.2",
    "rimraf": "^3.0.0",
    "standard-commit": "^3.2.0",
    "standard-version": "^7.0.0",
    "ts-jest": "^24.1.0",
    "tslint": "^5.20.0",
    "tslint-config-prettier": "^1.18.0",
    "tslint-plugin-prettier": "^2.0.1",
    "type-fest": "^0.8.1",
    "typescript": "^3.6.4",
    "yorkie": "^2.0.0"
  },
  "dependencies": {
    "cac": "^6.5.3",
    "chalk": "^2.4.2",
    "execa": "^3.2.0",
    "find-up": "^4.1.0",
    "fs-extra": "^8.1.0",
    "joycon": "^2.2.5",
    "js-yaml": "^3.13.1",
    "log-symbols": "^3.0.0",
    "loud-rejection": "^2.2.0",
    "normalize-package-data": "^2.5.0",
    "pacote": "^9.5.8",
    "read-pkg": "^5.2.0",
    "semver": "^6.3.0",
    "simple-git": "^3.5.0",
    "upath": "^1.2.0",
    "write-pkg": "^4.0.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/stasson/maintainr.git"
  },
  "bugs": {
    "url": "https://github.com/stasson/maintainr/issues"
  },
  "homepage": "https://github.com/stasson/maintainr#readme"
}
